  "scripts": {
    "dev": "nodemon --config nodem_dev.json",
    "build:client": "webpack --config ./client/build/webpack.base.js --env production --env platform=client  --progress",
    "build:client:dev": "webpack --config ./client/build/webpack.base.js --env development --env platform=client  --progress",
    "build:server": "webpack --config ./client/build/webpack.base.js --env production --env platform=server  --progress",
    "build:server:dev": "webpack --config ./client/build/webpack.base.js --env development --env platform=server  --progress",
    "build": "npm run type-check && npm run build:client && npm run build:server",
    "build:no-check": "npm run build:client && npm run build:server",
    "build:dev": "npm run build:client:dev && npm run build:server:dev",
    "type-check": "tsc --noEmit ",
    "type-check-watch": "tsc --noEmit --watch",
    "start": "nodemon --config nodem_prod.json"
  },
